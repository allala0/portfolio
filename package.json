  "main": "./build/index.min.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --mode development --open --hot",
    "explorer": "servez -p 8080 --ssl --hidden --no-index",
    "build": "cdir build && webpack build --mode production",
    "build-dev": "cdir build && webpack build --mode development",
    "build-module": "cdir build && webpack build --mode production --env module",
    "build-module-dev": "cdir build && webpack build --mode development --env module",
    "lint": "eslint src --ext js",
    "lint-fix": "eslint src --ext js --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "babel-loader": "^9.1.0",
    "clear-directory": "^1.0.7",
    "css-loader": "^6.7.2",
    "eslint": "^8.28.0",
    "eslint-plugin-react": "^7.31.11",
    "html-webpack-plugin": "^5.5.0",
    "sass-loader": "^13.2.0",
    "servez": "^1.14.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "three": "^0.146.0"
